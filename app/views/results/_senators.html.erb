      <div class="thumbnail">
        <%= image_tag s.image, :class=>"img-responsive", :alt=>s.name %>
        <div class="caption">
          <h3><%= s.name %></h3>
          <p>Estado: <%= s.state %></p>
          <p>Tel√©fono: <%= s.phone %></p>
          <p>Extensiones: <%= s.extention %></p>
          <p>Correo: <%= mail_to s.email %></p>
        </div>
      </div>
    </div>
  <% end %>
</div>
